{
    "name": "lelan/str-bundle",
    "type": "symfony-bundle",
    "description": "Google Pub/Sub transport for Symfony Messenger",
    "keywords": ["messenger", "symfony", "google", "pubsub"],
    "autoload": {
        "psr-4": {
            "Lelan\\StrBundle\\": ""
        }
    },
    "authors": [
        {
            "name": "leland",
            "email": "leland.beaumond@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "symfony/messenger": "^4.4|^5.1|^6.0",
        "symfony/options-resolver": "^4.4|^5.1|^6.0",
        "google/cloud-pubsub": "^1.29"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy-phpunit": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpstan/phpstan": "^0.12.64"
    }
}
